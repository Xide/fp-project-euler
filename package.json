{
  "name": "fp-project-euler",
  "version": "1.0.0",
  "description": "Functional programming solutions to the project Euler",
  "scripts": {
    "clean": "./node_modules/.bin/babel-changed --reset && rm -rf dist",
    "build": "./node_modules/.bin/babel-changed src -d dist",
    "test": "npm run test:all",
    "test:all": "npm run test:unit && npm run test:validate",
    "test:unit": "npm run build && ./node_modules/.bin/mocha dist/specs/*.js",
    "test:validate": "npm run build && ./node_modules/.bin/mocha dist/answers_check.js --timeout 60000"
  },
  "author": "Germain GAU",
  "license": "MIT",
  "dependencies": {
    "lazy.js": "^0.5.0",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "babel-changed": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "mocha": "^4.0.1"
  }
}
